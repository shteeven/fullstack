--
-- TOC entry 283 (class 1259 OID 74085)
-- Name: omw; Type: VIEW; Schema: public; Owner: Shtav
--

CREATE VIEW omw AS
 SELECT m.player_id,
    sum(m.match_outcome) AS match_points
   FROM matches m,
    players p
  WHERE ((m.player_id = p.id) AND (m.tourney_id = 2))
  GROUP BY m.player_id, p.name
  ORDER BY sum(m.match_outcome) DESC;


ALTER TABLE omw OWNER TO "Shtav";

--
-- TOC entry 284 (class 1259 OID 76984)
-- Name: opponents; Type: VIEW; Schema: public; Owner: Shtav
--

CREATE VIEW opponents AS
 SELECT p.id,
    abs((( SELECT sum(m_1.match_outcome) AS sum
           FROM matches m_1
          WHERE (p.id = m_1.player_id)) - ( SELECT sum(m_1.match_outcome) AS sum
           FROM matches m_1
          WHERE (3500 = m_1.player_id)))) AS diff
   FROM players p,
    matches m
  WHERE ((p.id <> 3500) AND (NOT (p.id IN ( SELECT matches.opponent_id
           FROM matches
          WHERE ((matches.player_id = 3500) AND (matches.tourney_id = 2))))))
  GROUP BY p.id
  ORDER BY abs((( SELECT sum(m_1.match_outcome) AS sum
           FROM matches m_1
          WHERE (p.id = m_1.player_id)) - ( SELECT sum(m_1.match_outcome) AS sum
           FROM matches m_1
          WHERE (3500 = m_1.player_id))));


ALTER TABLE opponents OWNER TO "Shtav";

